<mt:setvarblock name="page_title"><__trans phrase="Checkout [_1]" params="<mt:var name="object_label">"></mt:setvarblock>

<mt:setvarblock name="action_buttons">
  <button
     mt:mode="save_entry"
     type="submit"
     name="force_checkout"
     value="1"
     accesskey="s"
     title="<__trans phrase="Yes">"
     class="save action primary button">
    <__trans phrase="Yes">
  </button>
  <button
     mt:mode="view"
     type="submit"
     name="reedit"
     value="reedit"
     title="<__trans phrase="No">"
     class="action button">
    <__trans phrase="No">
  </button>
</mt:setvarblock>

<mt:include name="include/header.tmpl">

<form method="post" action="<mt:var name="script_url">">
    <input type="hidden" name="__mode" value="save_entry" />
    <input type="hidden" name="_type" value="<mt:var name="object_type">" />
    <input type="hidden" name="magic_token" value="<mt:var name="magic_token">" />
<mt:loop name="entry_loop">
    <input type="hidden" name="<mt:var name="data_name" escape="html">" value="<mt:var name="data_value" escape="html">" />
</mt:loop>

    <div class="error-message">
    <mtapp:statusmsg
       id="error-message"
       class="alert"
       can_close="0">
      <p><__trans phrase="It is not checked-out yet." /></p>
      <p><__trans phrase="Do you want to continue it after check-out?"></p>
    </mtapp:statusmsg>
    </div>
    <mt:include name="include/actions_bar.tmpl" hide_pager="1">
</form>

<mt:include name="include/footer.tmpl">
